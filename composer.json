{
    "name": "rxmg/php-sentiment",
    "type": "library",
    "description": "Sentiment analysis tool for PHP",
    "keywords": [
        "sentiment",
        "insight",
        "Sentiment analysis",
        "php"
    ],
    "homepage": "https://github.com/rxmg/rxpkg-php-sentiment",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": {
            "PHPInsight\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "generate-dictionaries": [
            "php lib/generate.php"
        ],
        "demo": [
            "php examples/demo.php"
        ],
        "post-install-cmd": [
            "php lib/generate.php"
        ],
        "post-update-cmd": [
            "php lib/generate.php"
        ]
    },
    "prefer-stable": true
}

